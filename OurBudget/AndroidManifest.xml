<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.BloodliviyKot.OurBudget"
          android:versionCode="2"
          android:versionName="1.1">
    <uses-sdk android:minSdkVersion="15"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application android:label="@string/app_name"
                 android:icon="@drawable/ic_launcher">
        <activity android:name=".WPurchases"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".WDetails"
                  android:label="@string/details_caption"
                  android:icon="@drawable/ic_pencil"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".WUserAccount"
                  android:label="@string/user_account_caption"
                  android:icon="@drawable/ic_authentication"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".WRegistration"
                  android:label="@string/registration_caption"
                  android:icon="@drawable/ic_add_authentication"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".WMarkTypes"
                  android:label="@string/mark_types_caption"
                  android:icon="@drawable/ic_g_tick"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".WTypes"
                  android:label="@string/types_caption"
                  android:icon="@drawable/ic_list_types"
                  android:screenOrientation="portrait">
        </activity>
        <service android:name="com.BloodliviyKot.synchronization.ServiceSynchronization"
                 android:enabled="true">
        </service>

    </application>
</manifest>
